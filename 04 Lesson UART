	//put into another terminal to see
	//cmd /c mode com3 baud=9600 parity=n data=8
	//cmd /c type com3



Code:
#include <avr/io.h>

#ifndef F_CPU
#define F_CPU 16000000UL
#endif
#include <util/delay.h>

#define BAUD 9600
#include <util/setbaud.h>


void uart_init(void)
{
	//baud rate generator
	//ensure the timing of pulses
	//corresponds to the expected bit rate
	UBRR0H = UBRRH_VALUE;
	UBRR0L = UBRRL_VALUE;

	//character size 8
	UCSR0C = UCSR0C | (1<<UCSZ01) | (1<<UCSZ00); //bitwise OR

	//transmitter enable
	UCSR0B = UCSR0B | (1<<TXEN0);
	return;
}

int main(void) {
	//configure UART
	uart_init();
	//send data
	//assign the ASCII code for A to UDR0
	UDR0 = 'A';

	return 0;
}
